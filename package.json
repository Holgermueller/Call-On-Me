    "url": "https://github.com/Holgermueller/student-manager-app/issues"
  },
  "homepage": "https://github.com/Holgermueller/student-manager-app#readme",
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.7",
    "morgan": "^1.9.1"
