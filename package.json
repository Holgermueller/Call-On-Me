    "url": "https://github.com/Holgermueller/student-manager-app/issues"
  },
  "homepage": "https://github.com/Holgermueller/student-manager-app#readme",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.0.3",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.29.0"
  }
